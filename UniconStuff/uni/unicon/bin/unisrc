zip -r uni.zip unicon/tests/special/Makefile unicon/tests/special/README
zip -r uni.zip unicon/tests/special/*.???
zip -r uni.zip unicon/tests/unicon/*.icn
zip -r uni.zip unicon/doc/bluelogo.gif
zip -r uni.zip unicon/doc/utr/utr12.html unicon/doc/utr/utr12/*.{jpg,png}
zip -r uni.zip unicon/doc/icon/ipd266.htm
