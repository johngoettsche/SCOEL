zip -r uni.zip unicon/src/rtt/*.[ch] unicon/src/rtt/Makefile
zip -r uni.zip unicon/src/runtime/*.r unicon/src/runtime/*.ri
zip -r uni.zip unicon/src/runtime/*.icn unicon/src/runtime/Makefile
zip -r uni.zip unicon/src/xpm/[ABD-Z]* unicon/src/xpm/xpm.h
zip -r uni.zip unicon/src/xpm/CHANGES unicon/src/xpm/COPYRIGHT
zip -r uni.zip unicon/src/xpm/doc/xpm.PS.gz unicon/src/xpm/lib/*.[ch]
zip -r uni.zip unicon/src/xpm/lib/*akefile* unicon/src/xpm/X11/xpm.h
zip -r uni.zip unicon/src/lib/voice/jvoip.h unicon/src/lib/audio/audio.h
zip -r uni.zip unicon/src/gdbm/*.c unicon/src/gdbm/*.h
